{
  "name": "cmesh_peer",
  "version": "0.0.1",
  "description": "Client lib for building peers for cmesh, a Ditto.live scale testing system.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepackage": "npm run clean && npm run build && node dist/package_setup.js",
    "package": "cd dist && npm pack",
    "clean": "rm -rf dist",
    "start": "tsc && node dist/index.js",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "test": "jest"
  },
  "files": [
    "dist/*"
  ],
  "type": "module",
  "repository": {
    "type": "git",
    "url": "github.com/getditto/orion/container-mesh"
  },
  "author": "DittoLive",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-typescript": "^7.23.3",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.17.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.5.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@dittolive/ditto": "^4.5.1"
  },
  "engines" : {
